{
  "name": "@ai-ninja/e2e-test-suite",
  "version": "1.0.0",
  "description": "Comprehensive E2E test suite for AI Phone Answering System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ai-ninja-e2e": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "node dist/index.js run",
    "test:suites": "node dist/index.js run --suites",
    "test:parallel": "node dist/index.js run --parallel --concurrency 4",
    "test:sequential": "node dist/index.js run --no-parallel",
    "test:verbose": "node dist/index.js run --verbose",
    "test:realtime": "node dist/index.js run --realtime",
    "plan": "node dist/index.js plan",
    "health": "node dist/index.js health",
    "config:generate": "node dist/index.js generate-config",
    "dev": "ts-node src/index.ts",
    "dev:run": "ts-node src/index.ts run --verbose",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist test-results coverage",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-ninja/ai-answer-ninja.git",
    "directory": "tests/e2e"
  },
  "keywords": [
    "e2e-testing",
    "integration-testing",
    "test-automation",
    "ai-phone-system",
    "parallel-testing",
    "test-reporting"
  ],
  "author": "AI Ninja Team",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "axios": "^1.11.0",
    "commander": "^11.1.0",
    "ws": "^8.14.2",
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended"
    ],
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/explicit-function-return-type": "off",
      "prefer-const": "error",
      "no-var": "error"
    },
    "env": {
      "node": true,
      "es2022": true
    }
  }
}