        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "avg": true
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [10],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "model": {
                  "expr": "pg_locks_count{datname=\"ai_ninja\", mode=\"AccessExclusiveLock\"}",
                  "refId": "A"
                },
                "params": ["A", "1m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "10s",
          "handler": 1,
          "name": "High Lock Count Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 14,
        "title": "PostgreSQL I/O Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 35
        },
        "targets": [
          {
            "expr": "rate(pg_stat_database_blks_read{datname=\"ai_ninja\"}[5m])",
            "refId": "A",
            "legendFormat": "Blocks Read/sec"
          },
          {
            "expr": "rate(pg_stat_database_blks_hit{datname=\"ai_ninja\"}[5m])",
            "refId": "B",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "pg_stat_database_blks_hit{datname=\"ai_ninja\"} / (pg_stat_database_blks_hit{datname=\"ai_ninja\"} + pg_stat_database_blks_read{datname=\"ai_ninja\"}) * 100",
            "refId": "C",
            "legendFormat": "Cache Hit Ratio %"
          }
        ],
        "yAxes": [
          {
            "label": "Blocks/sec, Hit Ratio %",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
