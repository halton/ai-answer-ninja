          },
          {
            "expr": "pg_settings_max_connections",
            "refId": "B",
            "legendFormat": "Max Connections"
          },
          {
            "expr": "pg_stat_activity_count{datname=\"ai_ninja\", state=\"idle\"}",
            "refId": "C",
            "legendFormat": "Idle Connections"
          },
          {
            "expr": "pg_stat_activity_count{datname=\"ai_ninja\", state=\"active\"}",
            "refId": "D",
            "legendFormat": "Active Queries"
          }
        ],
        "yAxes": [
          {
            "label": "Connection Count",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "avg": true
        },
        "thresholds": [
          {
            "value": 80,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 12,
        "title": "PostgreSQL Query Performance",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 27
        },
        "targets": [
          {
            "expr": "rate(pg_stat_database_tup_fetched{datname=\"ai_ninja\"}[5m])",
            "refId": "A",
            "legendFormat": "Tuples Fetched/sec"
          },
          {
            "expr": "rate(pg_stat_database_tup_inserted{datname=\"ai_ninja\"}[5m])",
            "refId": "B",
            "legendFormat": "Tuples Inserted/sec"
          },
          {
            "expr": "rate(pg_stat_database_tup_updated{datname=\"ai_ninja\"}[5m])",
            "refId": "C",
            "legendFormat": "Tuples Updated/sec"
          },
          {
            "expr": "rate(pg_stat_database_tup_deleted{datname=\"ai_ninja\"}[5m])",
            "refId": "D",
            "legendFormat": "Tuples Deleted/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Tuples per second",
            "logBase": 1,
            "max": null,
