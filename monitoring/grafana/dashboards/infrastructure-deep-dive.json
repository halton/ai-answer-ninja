      {
        "id": 13,
        "title": "PostgreSQL Lock Analysis",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 35
        },
        "targets": [
          {
            "expr": "pg_locks_count{datname=\"ai_ninja\", mode=\"AccessExclusiveLock\"}",
            "refId": "A",
            "legendFormat": "Exclusive Locks"
          },
          {
            "expr": "pg_locks_count{datname=\"ai_ninja\", mode=\"RowExclusiveLock\"}",
            "refId": "B",
            "legendFormat": "Row Exclusive Locks"
          },
          {
            "expr": "pg_locks_count{datname=\"ai_ninja\", mode=\"ShareLock\"}",
            "refId": "C",
            "legendFormat": "Share Locks"
          }
        ],
        "yAxes": [
          {
            "label": "Lock Count",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
