{
  "dashboard": {
    "id": null,
    "title": "AI Answer Ninja - Infrastructure Deep Dive",
    "tags": ["ai-ninja", "infrastructure", "detailed", "performance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 0,
    "time": {
      "from": "now-2h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Node Overview",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "collapsed": false
      },
      {
        "id": 2,
        "title": "CPU Usage by Node",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 1
        },
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "refId": "A",
            "legendFormat": "{{instance}}"
          }
        ],
        "yAxes": [
          {
            "label": "CPU Usage %",
            "logBase": 1,
            "max": 100,
            "min": 0,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "avg": true
        },
        "thresholds": [
          {
            "value": 80.0,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 3,
        "title": "Memory Usage by Node",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 1
        },
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "refId": "A",
            "legendFormat": "{{instance}} Memory Used"
          },
          {
            "expr": "(node_memory_Cached_bytes + node_memory_Buffers_bytes) / node_memory_MemTotal_bytes * 100",
            "refId": "B",
            "legendFormat": "{{instance}} Cached+Buffers"
          }
        ],
        "yAxes": [
          {
            "label": "Memory Usage %",
            "logBase": 1,
            "max": 100,
            "min": 0,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "avg": true
        },
        "thresholds": [
          {
            "value": 85.0,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "title": "Disk I/O",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 9
        },
        "targets": [
          {
            "expr": "rate(node_disk_read_bytes_total[5m])",
            "refId": "A",
            "legendFormat": "{{instance}} {{device}} Read"
          },
          {
            "expr": "rate(node_disk_written_bytes_total[5m])",
            "refId": "B",
            "legendFormat": "{{instance}} {{device}} Write"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes per second",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "avg": true
        }
      },
      {
        "id": 5,
        "title": "Network I/O",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 9
        },
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total{device!=\"lo\"}[5m])",
            "refId": "A",
            "legendFormat": "{{instance}} {{device}} Received"
