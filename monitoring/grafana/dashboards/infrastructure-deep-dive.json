        "yAxes": [
          {
            "label": "Bytes per second",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "avg": true
        }
      },
      {
        "id": 6,
        "title": "Kubernetes Resources",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 17
        },
        "collapsed": false
      },
      {
        "id": 7,
        "title": "Pod CPU Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 18
        },
        "targets": [
          {
            "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"ai-ninja\", container!=\"POD\", container!=\"\"}[5m])) by (pod)",
            "refId": "A",
            "legendFormat": "{{pod}}"
          }
        ],
        "yAxes": [
          {
            "label": "CPU Cores",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "avg": true,
          "rightSide": true
        }
      },
      {
        "id": 8,
        "title": "Pod Memory Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 18
        },
        "targets": [
          {
            "expr": "sum(container_memory_working_set_bytes{namespace=\"ai-ninja\", container!=\"POD\", container!=\"\"}) by (pod) / 1024 / 1024",
            "refId": "A",
            "legendFormat": "{{pod}}"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (MB)",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "avg": true,
          "rightSide": true
        }
      },
      {
        "id": 9,
        "title": "Pod Restart Count",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 18
        },
        "targets": [
          {
            "expr": "kube_pod_container_status_restarts_total{namespace=\"ai-ninja\"}",
            "refId": "A",
            "legendFormat": "{{pod}}/{{container}}"
          }
        ],
        "yAxes": [
          {
            "label": "Restart Count",
            "logBase": 1,
            "max": null,
            "min": 0,
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "current": true,
          "max": true,
          "total": true,
          "rightSide": true
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "model": {
                  "expr": "increase(kube_pod_container_status_restarts_total{namespace=\"ai-ninja\"}[10m])",
                  "refId": "A"
                },
                "params": ["A", "10m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "last"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "10s",
          "handler": 1,
          "name": "Pod Restart Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 10,
        "title": "Database Detailed Metrics",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 26
        },
        "collapsed": false
      },
      {
        "id": 11,
        "title": "PostgreSQL Connection Pool",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 27
        },
        "targets": [
          {
            "expr": "pg_stat_database_numbackends{datname=\"ai_ninja\"}",
            "refId": "A",
