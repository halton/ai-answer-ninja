        "gridPos": {
          "h": 4,
          "w": 3,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "avg(rate(ai_ninja_calls_successful[5m]) / rate(ai_ninja_calls_total[5m]) * 100)",
            "refId": "A"
