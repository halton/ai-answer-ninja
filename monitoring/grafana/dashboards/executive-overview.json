    "panels": [
      {
        "id": 1,
        "title": "System Health Score",
        "type": "singlestat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "min(\n  # Service availability (weight 40%)\n  (avg(up{job=~\"phone-gateway|realtime-processor|conversation-engine\"}) * 100) * 0.4 +\n  # Response time performance (weight 30%)\n  (1 - clamp_max(\n    histogram_quantile(0.95, rate(ai_response_latency_seconds_bucket[5m])) / 2.0, 1\n  )) * 100 * 0.3 +\n  # Call success rate (weight 20%)\n  (rate(ai_ninja_calls_successful[5m]) / rate(ai_ninja_calls_total[5m]) * 100) * 0.2 +\n  # Error rate inverse (weight 10%)\n  (1 - rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m])) * 100 * 0.1\n)",
            "refId": "A",
            "intervalFactor": 2
          }
        ],
        "thresholds": "70,85",
        "colorBackground": true,
        "colorValue": false,
        "colors": ["#d44a3a", "#e6c441", "#299c46"],
        "decimals": 1,
        "format": "percent",
        "gauge": {
          "show": true,
          "minValue": 0,
          "maxValue": 100,
          "thresholdLabels": false,
          "thresholdMarkers": true
        },
        "mappingTypes": [],
        "mappings": [],
        "nullPointMode": "connected",
        "sparkline": {
          "show": true,
          "full": false,
          "lineColor": "rgb(31, 120, 193)",
          "fillColor": "rgba(31, 118, 189, 0.18)"
        },
        "tableColumn": "",
        "unit": "percent",
        "valueFontSize": "80%",
        "valueMaps": [],
        "valueName": "current"
      },
      {
        "id": 2,
        "title": "Active Calls",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 3,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(ai_ninja_active_calls)",
            "refId": "A"
          }
        ],
        "colorBackground": false,
        "colorValue": true,
        "colors": ["#299c46", "#e6c441", "#d44a3a"],
        "decimals": 0,
        "format": "short",
        "mappingTypes": [],
        "mappings": [],
        "nullPointMode": "connected",
        "sparkline": {
          "show": true,
          "full": false,
          "lineColor": "rgb(31, 120, 193)",
          "fillColor": "rgba(31, 118, 189, 0.18)"
        },
        "thresholds": "50,100",
        "unit": "short",
        "valueFontSize": "80%",
        "valueMaps": [],
        "valueName": "current"
      },
      {
        "id": 3,
        "title": "Calls Per Hour",
        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 3,
          "x": 9,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(rate(ai_ninja_calls_total[1h])) * 3600",
            "refId": "A"
