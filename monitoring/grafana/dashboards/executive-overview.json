        ],
        "colorBackground": false,
        "colorValue": true,
        "colors": ["#d44a3a", "#e6c441", "#299c46"],
        "decimals": 1,
        "format": "percent",
        "sparkline": {
          "show": true,
          "full": false,
          "lineColor": "rgb(31, 120, 193)",
          "fillColor": "rgba(31, 118, 189, 0.18)"
        },
        "thresholds": "95,98",
        "unit": "percent",
        "valueFontSize": "80%",
        "valueName": "current"
      },
