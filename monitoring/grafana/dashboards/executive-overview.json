        ],
        "colorBackground": false,
        "colorValue": false,
        "colors": ["#299c46", "#e6c441", "#d44a3a"],
        "decimals": 0,
        "format": "short",
        "sparkline": {
          "show": true,
          "full": false,
          "lineColor": "rgb(31, 120, 193)",
          "fillColor": "rgba(31, 118, 189, 0.18)"
        },
        "unit": "short",
        "valueFontSize": "80%",
        "valueName": "current"
      },
      {
        "id": 4,
