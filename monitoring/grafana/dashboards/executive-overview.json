        "type": "singlestat",
        "gridPos": {
          "h": 4,
          "w": 3,
          "x": 18,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(ai_response_latency_seconds_bucket[5m])) by (le))",
            "refId": "A"
          }
        ],
        "colorBackground": false,
        "colorValue": true,
        "colors": ["#299c46", "#e6c441", "#d44a3a"],
        "decimals": 2,
        "format": "s",
        "sparkline": {
          "show": true,
          "full": false,
          "lineColor": "rgb(31, 120, 193)",
          "fillColor": "rgba(31, 118, 189, 0.18)"
