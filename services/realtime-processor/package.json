{
  "name": "realtime-processor",
  "version": "2.0.0",
  "description": "Enhanced real-time communication service with WebRTC, advanced audio processing, and performance optimization for AI Answer Ninja",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:enhanced": "node dist/enhancedServer.js",
    "start:realtime": "node dist/realtimeServer.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:enhanced": "ts-node-dev --respawn --transpile-only src/enhancedServer.ts",
    "dev:realtime": "ts-node-dev --respawn --transpile-only src/realtimeServer.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:communication": "node test-service-communication.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "client": "open examples/realtime-client.html || start examples/realtime-client.html",
    "clean": "rm -rf dist",
    "build:watch": "tsc -w"
  },
  "dependencies": {
    "@types/node": "^20.10.5",
    "ws": "^8.14.2",
    "redis": "^4.6.12",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "rate-limiter-flexible": "^5.0.3",
    "compression": "^1.7.4",
    "axios": "^1.6.2",
    "prom-client": "^15.1.0",
    "opossum": "^8.0.0",
    "eventemitter3": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.1.0",
    "ioredis": "^5.3.2",
    "p-queue": "^8.0.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.34.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "node-wav": "^0.0.2",
    "lamejs": "^1.2.1"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/fluent-ffmpeg": "^2.1.24",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}