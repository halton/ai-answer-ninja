    "rate-limiter-flexible": "^5.0.3",
    "compression": "^1.7.4",
    "axios": "^1.6.2",
    "prom-client": "^15.1.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.1.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
