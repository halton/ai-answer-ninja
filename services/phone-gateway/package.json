{
  "name": "phone-gateway",
  "version": "1.0.0",
  "description": "Phone Gateway Service for AI Answer Ninja - Handles incoming calls, routing, and filtering",
  "main": "dist/server.js",
  "author": "AI Answer Ninja Team",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "healthcheck": "node dist/utils/healthcheck.js",
    "docker:build": "docker build -t phone-gateway .",
    "docker:run": "docker run -p 3001:3001 phone-gateway"
  },
  "dependencies": {
    "@types/node": "^20.10.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "rate-limiter-flexible": "^5.0.3",
    "axios": "^1.6.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "bull": "^4.12.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "compression": "^1.7.4",
    "multer": "^1.4.5-lts.1",
    "@azure/communication-call-automation": "^1.2.0",
    "@azure/communication-common": "^2.3.0",
    "@azure/eventgrid": "^5.2.0",
    "@azure/storage-blob": "^12.17.0",
    "@azure/event-hubs": "^5.11.0",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bull": "^4.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "phone-gateway",
    "azure-communication-services",
    "call-routing",
    "spam-filtering",
    "ai-answer-ninja"
  ]
}